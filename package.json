{
  "name": "bkit-gui",
  "version": "1.3.22",
  "description": "A GUI for bKit Client",
  "productName": "bKit-gui",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Isidro Vila Verde <jvverde@gmail.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "build": "quasar build -m electron",
    "build-linux": "quasar build -m electron linux -b builder",
    "build-win32": "quasar build -m electron win32 -b builder",
    "build-mac": "quasar build -m electron mac -t mat -b builder",
    "dist-snap": "electron-builder --linux snap",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic"
  },
  "repository": "https://github.com/jvverde/bKit-gui.git",
  "keywords": [
    "bkit",
    "client",
    "gui",
    "backup",
    "time",
    "machine",
    "restore",
    "data",
    "recover",
    "cloud",
    "private"
  ],
  "dependencies": {
    "@growthbunker/vuedarkmode": "^0.5.56",
    "@quasar/extras": "^1.8.2",
    "chokidar": "^3.4.0",
    "electron-log": "^4.2.1",
    "electron-store": "^5.2.0",
    "electron-updater": "^4.3.1",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "node-powershell": "^4.0.0",
    "quasar": "^1.12.5",
    "upath": "^1.2.0",
    "vue-color": "^2.7.1",
    "vue-i18n": "^8.18.2",
    "windows-scheduler": "^1.0.1"
  },
  "devDependencies": {
    "@quasar/app": "^1.9.6",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.1.0-beta.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "devtron": "^1.4.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "majestic": "^1.7.0",
    "sinon": "^9.0.2",
    "wallaby-vue-compiler": "^1.0.3",
    "xvfb-maybe": "^0.2.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
